{# <h3 class="heading">Gallery<hr></h3> #}

<div class="cbanner pro-nepr container">
	<div class="row">
		{% for banner in banners %}
			<div class="col-xs-6">
				<div class="banner-e">
				  {% if banner.link %}
				  	<a href="{{ banner.link }}">
				  		<img src="{{ banner.image }}" alt="{{ banner.title }}" class="img-responsive center-block" />
				  	</a>
				  {% else %}
				  	<img src="{{ banner.image }}" alt="{{ banner.title }}" class="img-responsive center-block" />{% endif %}
				</div>
			</div>
		{% endfor %}
	</div>
</div>


